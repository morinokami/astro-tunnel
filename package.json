{
  "name": "astro-tunnel-root",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/morinokami/astro-tunnel.git"
  },
  "workspaces": [
    "packages/astro-tunnel",
    "playground"
  ],
  "scripts": {
    "dev": "bun run --cwd playground dev",
