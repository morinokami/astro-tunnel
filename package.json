{
  "name": "astro-integration-template",
  "description": "Template for Astro integrations",
  "version": "0.0.1",
  "type": "module",
  "author": "Shinya Fujino <shf0811@gamil.com> (https://github.com/morinokami)",
  "license": "MIT",
  "keywords": [
    "astro-integration"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
