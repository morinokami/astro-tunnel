    "build": "bun run --cwd packages/astro-tunnel build"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/changelog-github": "0.5.1",
    "@changesets/cli": "2.29.2",
    "pkg-pr-new": "0.0.53"
  }
}
